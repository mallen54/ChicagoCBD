<h1> Edit Subscription</h1>
<form action="/subscription/save" method="post">
    <input type="hidden" name="subscription_id" value="<%= onerec.subscription_id %>" >
<table>
    <tr> <td> Customer ID: </td> <td> <input type="number" name="customer_id" value="<%= onerec.customer_id %>" required> </td> </tr>
    <tr> <td> Category ID: </td> <td> <input type="number" name="category_id" value="<%= onerec.category_id %>" required> </td> </tr>
    <tr> <td> Subscribe Date: </td> <td> <input type="text" name="subscribedate" value="<%= onerec.subscribedate %>" required> </td> </tr>
    <tr> <td> Unsubscribe Date: </td> <td> <input type="text" name="unsubscribedate" value="<%= onerec.unsubscribedate %>" required> </td> </tr>
</table>
<button type="submit">Save</button>
</form>